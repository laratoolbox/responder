{
    "name": "LaraToolbox/responder",
    "description": "Easily return json responses for laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "responder"
    ],
    "type": "library",
    "authors": [
        {
            "name": "Semih ERDOGAN",
            "email": "hasansemiherdogan@gmail.com",
            "homepage": "https://semiherdogan.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5|^5.6|^5.7|^5.8|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5|^3.6|^3.7|^3.8|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "~6.0|^7.0|^7.5|^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaraToolbox\\responder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraToolbox\\responder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
      "laravel": {
          "providers": [
              "LaraToolbox\\responder\\PackageServiceProvider"
          ]
      }
  }
}
